{
  "scripts": {
    "clean": "rimraf publish",
    "lint": "eslint --ext .tsx,.ts . --quiet",
    "build": "npm run clean && npm run lint && cross-env NODE_ENV=production webpack --config config/webpack.dev.js",
    "buildBeta": "npm run clean && npm run lint && webpack --config config/webpack.dev-beta.js",
    "buildStaging": "npm run clean && npm run lint && webpack --config config/webpack.stag.js",
    "buildStagingBeta": "npm run clean && npm run lint && webpack --config config/webpack.stag-beta.js",
    "buildProduction": "npm run clean && npm run lint && webpack --config config/webpack.prod.js",
    "buildProductionBeta": "npm run clean && npm run lint && webpack --config config/webpack.prod-beta.js",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config config/webpack.dev.js --open --https --env dev",
    "stag": "webpack-dev-server --config config/webpack.stag.js --open --https"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "@svgr/webpack": "^5.5.0",
    "@types/react": "^17.0.19",
    "@types/react-datepicker": "^4.1.7",
    "@types/react-dom": "^17.0.9",
    "@types/react-redux": "^7.1.18",
    "@types/react-router": "^5.1.16",
    "@types/react-router-dom": "^5.1.8",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "babel-loader": "^8.2.2",
    "copy-webpack-plugin": "^10.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.2.0",
    "dotenv-webpack": "^7.0.3",
    "eslint": "^8.8.0",
    "eslint-plugin-react": "^7.26.1",
    "file-loader": "^6.2.0",
    "html-loader": "^3.1.0",
    "html-webpack-plugin": "^5.3.2",
    "react-imask": "^6.4.2",
    "redux": "^4.1.2",
    "sass": "^1.45.1",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.2.1",
    "ts-loader": "^9.2.5",
    "typesafe-actions": "^5.1.0",
    "typescript": "^4.3.5",
    "webpack": "^5.51.1",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.7.1"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.6.2",
    "axios": "^0.25.0",
    "classnames": "^2.3.1",
    "connected-react-router": "^6.9.1",
    "react": "^17.0.2",
    "react-day-picker": "^7.4.10",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.5",
    "react-router-dom": "^6.1.1"
  }
}
