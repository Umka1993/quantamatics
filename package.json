{
	"name": "quantamatics-ui",
	"private": true,
	"version": "1.5.0",
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build --mode testing",
		"preview": "vite preview --mode testing",
		"lint": "eslint --ext .tsx,.ts src --quiet",
		"lint:fix": "eslint --ext .tsx,.ts ./src --fix --color",
		"buildStaging": "npm run lint && tsc && vite build --mode staging",
		"buildUAT": "npm run lint && tsc && vite build --mode uat",
		"buildProduction": "npm run lint && tsc && vite build --mode production",
		"stag": "vite --mode preview"
	},
	"dependencies": {
		"axios": "^0.26.1",
		"classnames": "^2.3.1",
		"date-fns": "^2.28.0",
		"dialog-polyfill": "^0.5.6",
		"moment": "^2.29.1",
		"react": "^17.0.2",
		"react-datepicker": "^4.7.0",
		"react-day-picker": "^7.4.10",
		"react-dom": "^17.0.2",
		"react-redux": "^7.2.6",
		"react-router-dom": "^6.2.2"
	},
	"devDependencies": {
		"@reduxjs/toolkit": "^1.8.0",
		"@types/node": "^17.0.22",
		"@types/react": "^17.0.33",
		"@types/react-datepicker": "^4.3.4",
		"@types/react-dom": "^17.0.10",
		"@typescript-eslint/eslint-plugin": "^5.16.0",
		"@vitejs/plugin-legacy": "^1.7.1",
		"@vitejs/plugin-react": "^1.0.7",
		"eslint": "^8.11.0",
		"eslint-plugin-react": "^7.29.4",
		"husky": "^7.0.4",
		"lint-staged": "^12.3.7",
		"prettier": "^2.6.0",
		"sass": "^1.49.9",
		"typescript": "^4.5.4",
		"vite": "^2.8.0",
		"vite-plugin-svgr": "^1.1.0"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{ts,tsx}": [
			"lint:fix",
			"git add"
		]
	}
}
